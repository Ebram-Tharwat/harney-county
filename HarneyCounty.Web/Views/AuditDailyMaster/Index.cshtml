@model IEnumerable<HarneyCounty.Domain.Core.Models.DailyMaster>

@{
    ViewBag.Title = "Index";
}

<h2>Daily Master</h2>

<p>
    @Html.ActionLink("Create New", "CreateDailyMaster",new { id = ViewBag.TurnOverSequenceId })
</p>
<table class="table">
    <tr>
        <th>
            @Html.Display("Id")
        </th>
        <th>
            @Html.DisplayNameFor(model => model.AuditTurnoverSequenceId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.EntryDate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FromReceipt)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ThruReceipt)
        </th>
       
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            <a href="@Url.Action("Index", "AuditEditDailyTransaction",new { id = item.Id })">@item.Id</a>
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.AuditTurnoverSequenceId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.EntryDate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FromReceipt)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ThruReceipt)
        </td>
        @*<td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>*@
    </tr>
}

</table>
